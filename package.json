{
  "name": "@birxlabs/common",
  "version": "1.0.0",
  "description": "A Scaffold for creating typescript react packages",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "postinstall": "npm run build",
    "build": "tslint -c tslint.json 'src/**/*.ts' && rimraf ./dist/* && tsc --watch false",
    "watch": "tslint -c tslint.json 'src/**/*.ts' && rimraf ./dist/* && tsc --watch true",
    "clean": "rimraf build && rimraf node_modules",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "getstorybook": "getstorybook"
  },
  "author": "BirxDK",
  "license": "ISC",
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addon-links": "^3.3.15",
    "@storybook/addons": "^3.3.15",
    "@storybook/cli": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "@types/antd": "^1.0.0",
    "@types/enzyme": "^3.1.9",
    "@types/jest": "^22.2.1",
    "@types/node": "^9.4.7",
    "@types/react": "^16.0.40",
    "@types/react-test-renderer": "^16.0.1",
    "@types/storybook__react": "^3.0.7",
    "awesome-typescript-loader": "^4.0.1",
    "babel-core": "^6.26.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.4.3",
    "jest-styled-components": "^5.0.1",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.9.1",
    "tslint-react": "^3.5.1",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "styled-components": "^3.2.3"
  }
}
